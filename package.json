{
  "name": "angular2-translate",
  "version": "3.0.1",
  "description": "Simple translate for angular2",
  "bin": {
    "semantic-release": "bin/semantic-release.js"
  },
  "keywords": [
    "angular",
    "angular2",
    "translate",
    "i18n"
  ],
  "main": "dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "test": "mocha --require es6-shim --require es6-promise --require reflect-metadata --require ts-node/register lib/**/*.spec.ts",
    "coverage": "npm test && nyc report --reporter=text-lcov | coveralls",
    "prepublish": "typings install && rimraf dist && tsc",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "release": {
    "verifyConditions": {
      "path": "./node_modules/semantic-release/dist/lib/plugin-verify.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Valetudox/angular2-translate.git"
  },
  "author": "Viktor Somodi <viktor.somodi@emarsys.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Valetudox/angular2-translate/issues"
  },
  "homepage": "https://github.com/Valetudox/angular2-translate#readme",
  "dependencies": {
    "sprintf-js": "^1.0.3",
    "flat": "^1.6.0"
  },
  "devDependencies": {
    "angular2": "2.0.0-beta.13",
    "chai": "3.5.0",
    "es6-promise": "3.1.2",
    "es6-shim": "0.35.0",
    "mocha": "2.4.5",
    "reflect-metadata": "0.1.2",
    "rimraf": "2.5.2",
    "rxjs": "5.0.0-beta.2",
    "semantic-release": "4.3.5",
    "ts-node": "0.7.0",
    "typescript": "1.8.9",
    "typings": "0.7.9",
    "zone.js": "0.6.5"
  }
}
